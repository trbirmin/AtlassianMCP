<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="dist/server.js" verb="*" modules="iisnode"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="DynamicContent">
          <match url="/*" />
          <action type="Rewrite" url="dist/server.js" />
        </rule>
      </rules>
    </rewrite>
    <httpProtocol>
      <customHeaders>
        <add name="X-Powered-By" value="" />
      </customHeaders>
    </httpProtocol>
    <iisnode
      node_process_commandline="%programfiles%\\nodejs\\node.exe"
      flushResponse="true"
      devErrorsEnabled="false"
    />
  </system.webServer>
</configuration>
